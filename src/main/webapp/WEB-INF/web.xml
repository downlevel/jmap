<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>jmap</display-name>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>markers</servlet-name>
    <servlet-class>it.downlevel.servlets.MarkerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>categories</servlet-name>
    <servlet-class>it.downlevel.servlets.CategoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>markers</servlet-name>
    <url-pattern>/markers</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>categories</servlet-name>
    <url-pattern>/categories</url-pattern>
  </servlet-mapping>
</web-app>